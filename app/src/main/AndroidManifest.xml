<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.domi.fightlapyapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:debuggable="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".zawodniczkaActivities.WyborZawodniczkiActivity"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>

        <activity
            android:name=".zawodniczkaActivities.ListaZawodniczekActivity"
            android:parentActivityName=".zawodniczkaActivities.WyborZawodniczkiActivity" />
        <activity
            android:name=".zawodniczkaActivities.DodanieZawodniczkiActivity"
            android:parentActivityName=".zawodniczkaActivities.WyborZawodniczkiActivity" />
        <activity
            android:name=".zawodniczkaActivities.UdanyZapisZawodniczkiActivity"
            android:parentActivityName=".zawodniczkaActivities.WyborZawodniczkiActivity" />
        <activity
            android:name=".wydarzenieActivities.WyborWydarzeniaActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".wydarzenieActivities.DodanieWydarzeniaActivity"
            android:parentActivityName=".wydarzenieActivities.WyborWydarzeniaActivity" />
        <activity
            android:name=".wydarzenieActivities.UdanyZapisWydarzenieActivity"
            android:parentActivityName=".wydarzenieActivities.WyborWydarzeniaActivity" />
        <activity
            android:name=".wydarzenieActivities.ListaWydarzenActivity"
            android:parentActivityName=".wydarzenieActivities.WyborWydarzeniaActivity" />
        <activity
            android:name=".ZapisZawodniczkiNaWydarzenieActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".zawodniczkaActivities.WybranaZawodniczkaZListyActivity"
            android:parentActivityName=".zawodniczkaActivities.ListaZawodniczekActivity" />
        <activity
            android:name=".UdanyZapisZawodniczkiNaWydarzenieActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".wydarzenieActivities.WybraneWydarzenieZListyActivity"
            android:parentActivityName=".wydarzenieActivities.WyborWydarzeniaActivity" />
        <activity
            android:name=".wydarzenieActivities.ListaWydarzenEdycjaActivity"
            android:parentActivityName=".wydarzenieActivities.WyborWydarzeniaActivity" />
        <activity
            android:name=".wydarzenieActivities.WybraneWydarzenieZListyEdycjaActivity"
            android:parentActivityName=".wydarzenieActivities.WyborWydarzeniaActivity" />
        <activity
            android:name=".wydarzenieActivities.UdanaEdycjaWydarzeniaActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".wydarzenieActivities.ListaZapisanychZawodniczekActivity"
            android:parentActivityName=".wydarzenieActivities.ListaWydarzenEdycjaActivity" />
        <activity
            android:name=".zawodniczkaActivities.ListaZawodniczekEdycjaActivity"
            android:parentActivityName=".zawodniczkaActivities.WyborZawodniczkiActivity" />
        <activity
            android:name=".zawodniczkaActivities.WybranaZawodniczkaZListyEdycjaActivity"
            android:parentActivityName=".zawodniczkaActivities.ListaZawodniczekEdycjaActivity" />
        <activity
            android:name=".zawodniczkaActivities.UdanaEdycjaZawodniczkiActivity"
            android:parentActivityName=".zawodniczkaActivities.ListaZawodniczekEdycjaActivity" />
        <activity
            android:name=".FacebookActivity"
            android:parentActivityName=".MainActivity" />

        <meta-data
            android:name="com.facebook.sdk.ApplicationId"
            android:value="@string/facebook_app_id" />

        <activity android:name=".AfterLoggingActivity"
            android:parentActivityName=".MainActivity"/>

        <activity android:name=".userActivities.ZapisUseraNaWydarzenieActivity"
            android:parentActivityName=".MainActivity"/>

        <activity android:name=".userActivities.EdycjaDanychUsera"
            android:parentActivityName=".MainActivity"/>

    </application>

</manifest>