<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.domi.fightlapyapp.WyborZawodniczkiActivity">

    <Button
        android:id="@+id/nowe_wydarzenie"
        android:layout_width="192dp"
        android:layout_height="85dp"
        android:onClick="dodanieZawodniczki"
        android:text="@string/dodaj_zawodniczke"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="8dp" />

    <Button
        android:id="@+id/lista_zawodniczek"
        android:layout_width="192dp"
        android:layout_height="85dp"
        android:layout_marginTop="8dp"
        android:onClick="listaZawodniczek"
        android:text="@string/lista_zawodniczek"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/nowe_wydarzenie" />

    <Button
        android:id="@+id/edytuj_zawodniczke"
        android:layout_width="192dp"
        android:layout_height="85dp"
        android:layout_marginTop="8dp"
        android:onClick="edycjaZawodniczki"
        android:text="@string/edycja_zawodniczki"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/lista_zawodniczek" />
</android.support.constraint.ConstraintLayout>
